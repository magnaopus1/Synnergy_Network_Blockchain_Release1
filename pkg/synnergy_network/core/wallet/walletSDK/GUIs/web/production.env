# Blockchain Configuration
# Example: Ethereum Mainnet RPC URL
REACT_APP_BLOCKCHAIN_RPC_URL=https://mainnet.infura.io/v3/YOUR-PROJECT-ID

# WebSocket URL for Real-Time Notifications and Updates
REACT_APP_WS_URL=wss://your_websocket_url

# Encryption Key for Data Security
REACT_APP_ENCRYPTION_KEY=your_encryption_key

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#007BFF
REACT_APP_SECONDARY_COLOR=#6c757d
REACT_APP_BACKGROUND_COLOR=#f5f5f5
REACT_APP_TEXT_COLOR=#333

# Storage Configuration
REACT_APP_LOCAL_BACKUP_PATH=/path/to/local/backup
REACT_APP_CLOUD_BACKUP_PROVIDER_URL=https://cloud-backup-provider-url.com

# Hardware Security Module (HSM) Configuration
REACT_APP_HSM_MODULE_PATH=/path/to/pkcs11/module
REACT_APP_HSM_PIN=your_hsm_pin

# Cross-Chain Integration URL
REACT_APP_CROSS_CHAIN_INTEGRATION_URL=https://cross-chain-integration-url.com
