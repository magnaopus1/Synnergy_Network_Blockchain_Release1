# Local Blockchain Configuration
REACT_APP_BLOCKCHAIN_RPC_URL=http://localhost:8545

# WebSocket URL for Real-Time Notifications and Updates
REACT_APP_WS_URL=ws://localhost:8546

# Encryption Key for Data Security
REACT_APP_ENCRYPTION_KEY=your_local_encryption_key

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#007BFF
REACT_APP_SECONDARY_COLOR=#6c757d
REACT_APP_BACKGROUND_COLOR=#f5f5f5
REACT_APP_TEXT_COLOR=#333

# Storage Configuration
REACT_APP_LOCAL_BACKUP_PATH=/path/to/local/backup
REACT_APP_CLOUD_BACKUP_PROVIDER_URL=http://localhost:9000

# Hardware Security Module (HSM) Configuration
REACT_APP_HSM_MODULE_PATH=/path/to/local/pkcs11/module
REACT_APP_HSM_PIN=your_local_hsm_pin

# Cross-Chain Integration URL
REACT_APP_CROSS_CHAIN_INTEGRATION_URL=http://localhost:8547
